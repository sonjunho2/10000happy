{
  "compilerOptions": {
    "target": "es2017",
    "lib": [
      "dom",
      "es2017"
    ],
    "allowJs": true,
    "jsx": "react",
    "moduleResolution": "node",
    "noEmit": true,
    "strict": true,
    "resolveJsonModule": true,
    "baseUrl": ".",
    "paths": {
      "*": [
        "./*",
        "src/*"
      ]
    }
  },
  "exclude": [
    "node_modules"
  ]
}